{
  "name": "ns-cms-front",
  "version": "1.0.0",
  "author": "asasinmode",
  "description": "vue 3 admin site for ns-cms",
  "license": "MIT",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview --port 8173",
    "test:e2e": "start-server-and-test preview http://localhost:8173/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://localhost:8173/ 'cypress run --e2e'",
    "test:unit": "cypress open --component",
    "test:unit:ci": "cypress run --component --quiet --reporter spec"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@fortawesome/vue-fontawesome": "^3.0.1",
    "axios": "^0.27.2",
    "pinia": "^2.0.17",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@types/node": "^16.11.47",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.8",
    "cypress": "^10.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8",
    "typescript": "~4.7.4",
    "start-server-and-test": "^1.14.0",
    "vite": "^3.0.4"
  }
}
